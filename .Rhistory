library(ggplot2)
data("mpg")
hist_with_normal_curve <- function(bins) {
ggplot(mpg, aes(x = hwy)) +
geom_histogram(aes(y = density), bins = bins, fill = "skyblue", color = "black", aplha = 0.7) +
stat_function(fun = dnorm, args = list(mean = mean(mpg$hwy, na.rm=TRUE),
sd = sd(mpg$hwy, na.rm=TRUE)),
color = "red", size = 1) +
labs(title = paste("Histogram of hwy with Normal Curve (bins = ", bins, ")"),
x = "Highway Miles per Gallon", y = "Density") +
theme_minimal()
}
hist_with_normal_curve(10)
